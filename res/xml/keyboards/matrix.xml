<keyboard>
	<key name='close' row='1' col='1' default-color='#333'>
		<primary close='close' text-label='close'/>
	</key>
	<key name='1x1' row='1' col='2'>
		<primary close='close' action='input: left' text-label='[ • ]'>\matrixOneOne{}</primary>
	</key>
	<key name='1x2' row='1' col='3'>
		<primary close='close' action='input: left,left' text-label='[ •• ]'>\matrixOneTwo{}{}</primary>
	</key>
	<key name='1x3' row='1' col='4'>
		<primary close='close' action='input: left,left,left' text-label='[ ••• ]'>\matrixOneThree{}{}{}</primary>
	</key>
	<key name='2x1' row='2' col='2'>
		<primary close='close' action='input: left,left' text-label='[ : ]'>\matrixTwoOne{}{}</primary>
	</key>
	<key name='2x2' row='2' col='3'>
		<primary close='close' action='input: left,left,left,left' text-label='[ :: ]'>\matrixTwoTwo{}{}{}{}</primary>
	</key>
	<key name='2x3' row='2' col='4'>
		<primary close='close' action='input: left,left,left,left,left,left' text-label='[ ::: ]'>\matrixTwoThree{}{}{}{}{}{}</primary>
	</key>
	<key name='3x1' row='3' col='2'>
		<primary close='close' action='input: left,left,left' style='letter-spacing:-0.15em;font-size:30px' text-label='[ ⋮ ]'>\matrixThreeOne{}{}{}</primary>
	</key>
	<key name='3x2' row='3' col='3'>
		<primary close='close' action='input: left,left,left,left,left,left' style='letter-spacing:-0.15em;font-size:30px' text-label='[ ⋮⋮ ]'>\matrixThreeTwo{}{}{}{}{}{}</primary>
	</key>
	<key name='3x3' row='3' col='4'>
		<primary close='close' action='input: left,left,left,left,left,left,left,left,left' style='letter-spacing:-0.2em;font-size:30px' text-label='[ ⋮⋮⋮ ]'>\matrixThreeThree{}{}{}{}{}{}{}{}{}</primary>
	</key>
	<key name='det' row='4' col='2'>
		<primary close='close' action='input:left' text-label='det'>\det()</primary>
	</key>
	<key name='trans' row='4' col='3'>
		<primary close='close' action='input:left' text-label='trans'>\transpose()</primary>
	</key>
	<key name='id' row='4' col='4'>
		<primary close='close' action='input:left' text-label='id'>\id()</primary>
	</key>
	<key name='inverse' row='5' col='2'>
		<primary close='close' action='input:left' text-label='inv'>\inverse()</primary>
	</key>
	<key name='brackets' row='5' col='3'>
		<primary close='close' action='input:left' text-label='[ ]'>\left[\right]</primary>
	</key>
	<key name='bar' row='5' col='4'>
		<primary close='close' text-label='|'>|</primary>
	</key>
</keyboard>